#include <iostream>
using namespace std;

int main() {
	int n;
	cin>>n;
	int ar[n];
	for(int i=0;i<n;i++)
	{
		cin>>ar[i];
	}
	int temp=0;
		for (int i=0 ; i<n; i++)
		{
			for(int j= 0; j<n; j++)
			{
				if(ar[i]<ar[j])
				{
				temp=ar[i];
				ar[j] = ar[i];
				ar[i]=temp;
				cout<<ar[i];
				}
			}
		}

	return 0;
}
